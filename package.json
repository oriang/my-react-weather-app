{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate for react apps using browserify",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "browserify src/app.jsx -t [ babelify --presets [ es2015 react stage-0 ] ] -o public/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "orian gonzalves",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0"
  }
}
